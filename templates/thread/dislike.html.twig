{% if disliked %}
    <i id="thread_dislike{{ thread_id }}" class="fa fa-thumbs-down float-right p-1 dislike-btn" data-id="{{ thread_id }}" style="font-size:24px"></i>
{% else %}
    <i id="thread_dislike{{ thread_id }}" class="fa fa-thumbs-o-down float-right p-1 dislike-btn" data-id="{{ thread_id }}" style="font-size:24px"></i>
{% endif %}