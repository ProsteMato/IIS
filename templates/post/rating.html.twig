

{% if liked %}
    <i class="fa fa-thumbs-up float-left p-1 like-btn" data-id="{{ post_id }}" style="font-size:24px"></i>
{% else %}
    <i class="fa fa-thumbs-o-up float-left p-1 like-btn" data-id="{{ post_id }}" style="font-size:24px"></i>
{% endif %}

{% if disliked %}
    <i class="fa fa-thumbs-down float-left p-1 dislike-btn" data-id="{{ post_id }}" style="font-size:24px"></i>
{% else %}
    <i class="fa fa-thumbs-o-down float-left p-1 dislike-btn" data-id="{{ post_id }}" style="font-size:24px"></i>
{% endif %}
